{
  "name": "Modern Teal Analytics Theme",
  "dataColors": [
    "#00A6A6", "#1EAE98", "#2B2D42", "#5C6B73", "#8ED1C2", "#E0F7F4"
  ],
  "background": "#F9FAFB",
  "foreground": "#2B2D42",
  "tableAccent": "#00A6A6",
  "visualStyles": {
    "*": {
      "*": {
        "title": [
          { "color": { "solid": { "color": "#2B2D42" } }, "fontSize": 18, "fontFamily": "Segoe UI Semibold" }
        ],
        "subtitle": [
          { "color": { "solid": { "color": "#555555" } }, "fontSize": 12, "fontFamily": "Segoe UI" }
        ],
        "labels": [
          { "color": { "solid": { "color": "#555555" } }, "fontSize": 10, "fontFamily": "Segoe UI" }
        ],
        "background": [
          { "color": { "solid": { "color": "#FFFFFF" } } }
        ],
        "border": [
          { "color": { "solid": { "color": "#E0E0E0" } } }
        ]
      }
    },
    "visualContainer": {
      "*": {
        "shadow": [{ "show": true }]
      }
    }
  },
  "textClasses": {
    "title": { "fontFamily": "Segoe UI Semibold", "fontSize": 18, "color": "#2B2D42" },
    "header": { "fontFamily": "Segoe UI", "fontSize": 12, "color": "#555555" },
    "body": { "fontFamily": "Segoe UI", "fontSize": 10, "color": "#555555" }
  }
}
